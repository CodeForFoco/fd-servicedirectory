		"axios": "0.21.1",
		"husky": "^2.4.0",
		"lodash": "^4.17.13",
		"polished": "^3.3.0",
		"qs": "^6.7.0",
		"react": "^16.8.3",
		"react-dom": "^16.8.6",
		"react-redux": "^7.1.3",
		"react-router-dom": "^5.0.0",
		"redux": "^4.0.5",
		"redux-thunk": "^2.3.0",
		"styled-components": "^4.2.0",
		"styled-normalize": "^8.0.6",
		"ts-jest": "^24.1.0"
	}
